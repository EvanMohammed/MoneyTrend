{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "globals": {"$" : true},
    "rules": {
        "import/no-unresolved": "off",
        "func-names": "off", 
        "no-param-reassign": [2, { "props": false }]
    }
}
